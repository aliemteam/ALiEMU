mixin user(name, img, title, role, twitter)
    .au-team-member
        .au-photo
            img(src="/wp-content/uploads/#{img}")
        .au-about
            .au-name
                if twitter
                    .au-social
                        a(href="https://twitter.com/#{twitter}" class="et_pb_font_icon et_pb_twitter_icon")
                h4=name
            .au-title=title
            .au-role=role
- placeholder_img = '2015/04/placeholder.jpeg'
- get_header()

