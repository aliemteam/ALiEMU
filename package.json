{
  "name": "aliemu",
  "version": "1.0.0",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "start": "gulp serve",
    "wp": "wpcmd() { docker exec $(docker ps -lq) /bin/bash -c \"sudo -u www-data wp $(echo $@)\"; };wpcmd ",
    "pretest": "npm run lint",
    "clear-cache": "rm -rf node_modules/jest-cli/.haste_cache",
    "deploy": "./scripts/deploy",
    "rollback": "if [[ $(git status --porcelain) ]]; then git stash; fi && git checkout master && git fetch && git pull && git revert HEAD --no-edit && npm run deploy && git stash pop",
    "test": "jest",
    "lint": "stylint wp-content/plugins/aliemu-plugins/inc/styles -c .stylintrc && tslint 'wp-content/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@types/enzyme": "^1.2.23-alpha",
    "@types/jest": "^0.9.21-alpha",
    "@types/moment": "^2.11.22-alpha",
    "@types/react": "^0.14.21-alpha",
    "@types/react-addons-test-utils": "^0.14.8-alpha",
    "@types/react-dom": "^0.14.8-alpha",
    "@types/sinon": "^1.16.20-alpha",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-datepicker": "^0.27.0",
    "react-dom": "^15.1.0",
    "typescript": "^1.9.0-dev.20160625-1.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "browser-sync": "^2.12.10",
    "coveralls": "^2.11.9",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "enzyme": "^2.3.0",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.5.0",
    "gulp-uglify": "^1.5.4",
    "jest-cli": "^13.0.0",
    "poststylus": "^0.2.3",
    "react-addons-test-utils": "^15.1.0",
    "rucksack-css": "^0.8.6",
    "sinon": "^1.17.4",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.5",
    "ts-loader": "^0.8.1",
    "tslint": "^3.11.0",
    "webpack": "^1.13.1",
    "webpack-stream": "^3.1.0"
  },
  "jest": {
    "scriptPreprocessor": "jestPreprocessor.js",
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme",
      "sinon",
      "<rootDir>/test-utils/Fixtures"
    ],
    "verbose": true,
    "collectCoverage": true
  }
}
