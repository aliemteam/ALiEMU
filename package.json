{
    "name": "aliemu.com",
    "version": "2.1.0",
    "license": "MIT",
    "repository": "https://github.com/aliemteam/ALiEMU",
    "scripts": {
        "build": "TS_NODE_PROJECT=./tsconfig.jest.json NODE_ENV=production webpack",
        "lint:scss": "stylelint \"src/**/*.scss\"",
        "lint:ts": "tslint -p . -t stylish",
        "lint:php": "npm run phpcs -- --ignore=src/learndash --report=${PHPCS_REPORT:-summary} --basepath=. ./src",
        "phpcs": "docker run -it --rm -v $(pwd):/app dsifford/phpcs",
        "lint": "npm run lint:scss && npm run lint:ts && npm run lint:php",
        "pretest": "PHPCS_REPORT=full npm run lint",
        "start": "TS_NODE_PROJECT=./tsconfig.jest.json webpack",
        "sync": "./lib/sync.sh",
        "test": "jest --coverage",
        "view-coverage": "$(test $(uname) == Linux && echo xdg-open || echo open) ./node_modules/.cache/coverage/lcov-report/index.html",
        "up": "docker-compose up -d && docker-compose logs -f wordpress",
        "down": "docker-compose down -v"
    },
    "dependencies": {
        "@babel/runtime": "^7.1.2",
        "classnames": "^2.2.6",
        "datalist-polyfill": "^1.22.1",
        "date-fns": "^1.29.0",
        "mobx": "^4.5.2",
        "mobx-react": "^5.3.6",
        "mobx-utils": "^5.0.4",
        "normalize.css": "^8.0.0",
        "react": "^16.6.0",
        "react-content-loader": "^3.2.0",
        "react-dom": "^16.6.0",
        "react-spring": "^6.1.6",
        "striptags": "^3.1.1",
        "typescript": "^3.1.6"
    },
    "devDependencies": {
        "@babel/core": "^7.1.2",
        "@babel/plugin-transform-runtime": "^7.1.0",
        "@babel/preset-env": "^7.1.0",
        "@babel/preset-react": "^7.0.0",
        "@dsifford/stylelint-config": "^0.2.0",
        "@svgr/webpack": "^3.1.0",
        "@types/classnames": "^2.2.6",
        "@types/copy-webpack-plugin": "^4.4.2",
        "@types/dotenv": "^4.0.3",
        "@types/enzyme": "^3.1.14",
        "@types/enzyme-adapter-react-16": "^1.0.3",
        "@types/googlemaps": "^3.30.16",
        "@types/grecaptcha": "^2.0.35",
        "@types/jest": "^23.3.9",
        "@types/jquery": "^3.3.22",
        "@types/mini-css-extract-plugin": "^0.2.0",
        "@types/node": "^10.12.2",
        "@types/react": "^16.4.18",
        "@types/react-addons-test-utils": "^0.14.22",
        "@types/react-content-loader": "^3.1.3",
        "@types/react-dom": "^16.0.9",
        "@types/react-test-renderer": "^16.0.3",
        "@types/webpack": "^4.4.17",
        "awesome-typescript-loader": "^5.2.1",
        "babel-loader": "^8.0.4",
        "browser-sync": "^2.26.3",
        "browser-sync-webpack-plugin": "^2.2.2",
        "copy-webpack-plugin": "^4.6.0",
        "css-loader": "^1.0.1",
        "dotenv": "^6.1.0",
        "enzyme": "^3.7.0",
        "enzyme-adapter-react-16": "^1.6.0",
        "identity-obj-proxy": "^3.0.0",
        "imagemin": "^6.0.0",
        "imagemin-pngquant": "^6.0.0",
        "imagemin-svgo": "^7.0.0",
        "jest": "^23.6.0",
        "mini-css-extract-plugin": "^0.4.4",
        "mobx-react-devtools": "^6.0.3",
        "node-sass": "^4.9.4",
        "prettier": "^1.14.3",
        "react-addons-test-utils": "^15.6.2",
        "react-test-renderer": "^16.6.0",
        "sass-loader": "^7.1.0",
        "snapshot-diff": "^0.4.0",
        "style-loader": "^0.23.1",
        "stylelint": "^9.7.1",
        "stylelint-scss": "^3.3.2",
        "ts-jest": "^23.10.4",
        "ts-node": "^7.0.1",
        "tsconfig-paths": "^3.6.0",
        "tslint": "^5.11.0",
        "tslint-config-prettier": "^1.15.0",
        "tslint-microsoft-contrib": "^5.2.1",
        "tslint-react": "^3.6.0",
        "webpack": "^4.24.0",
        "webpack-cli": "^3.1.2"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx}",
            "!**/{vendor,__tests__}/**"
        ],
        "coverageDirectory": "node_modules/.cache/coverage",
        "globals": {
            "ts-jest": {
                "tsConfig": "tsconfig.jest.json"
            }
        },
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy",
            "^(utils|components)/(.*)$": "<rootDir>/src/js/$1/$2"
        },
        "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
        "testURL": "http://localhost",
        "transform": {
            ".(ts|tsx)": "ts-jest"
        }
    }
}
