{
  "name": "aliemu",
  "version": "1.3.2",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "lint": "npm run lint:stylint && npm run lint:tslint",
    "lint:stylint": "stylint aliemu/styles -c .stylintrc",
    "lint:tslint": "tslint -p . -t codeFrame",
    "build": "NODE_ENV=production gulp",
    "start": "node --max_old_space_size=8192 ./node_modules/.bin/gulp",
    "deploy": "yarn test && yarn build && ./lib/scripts/deploy",
    "deploy-all": "npm run test && npm run build && ./lib/scripts/deploy -a",
    "sync": "./lib/scripts/sync",
    "pretest": "yarn lint",
    "test": "jest",
    "version": "gulp bump"
  },
  "dependencies": {
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "moment": "^2.17.1",
    "react": "^15.6.1",
    "react-datepicker": "^0.52.0",
    "react-dom": "^15.6.1",
    "typescript": "^2.4.2"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.3",
    "@types/jest": "^20.0.4",
    "@types/node": "^8.0.14",
    "@types/react": "^15.0.38",
    "@types/react-addons-test-utils": "^0.14.19",
    "@types/react-datepicker": "^0.46.2",
    "@types/react-dom": "^15.5.1",
    "@types/webpack": "^3.0.4",
    "autoprefixer-stylus": "^0.14.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "browser-sync": "^2.18.12",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "jest-cli": "^20.0.4",
    "merge-stream": "^1.0.0",
    "mobx-react-devtools": "^4.2.15",
    "react-addons-test-utils": "^15.6.0",
    "style-loader": "^0.18.2",
    "stylint": "^1.5.9",
    "ts-loader": "^2.3.1",
    "ts-node": "^3.2.0",
    "tslint": "^5.5.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "tslint-react": "^3.0.0",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-stream": "^3.2.0"
  },
  "jest": {
    "transform": {
      ".*": "./lib/utils/jestPreprocessor.js"
    },
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "verbose": true,
    "collectCoverageFrom": [
      "aliemu/**/*.{ts,tsx}",
      "!aliemu/aliemu.d.ts",
      "!aliemu/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ]
  }
}
