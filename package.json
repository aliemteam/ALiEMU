{
  "name": "aliemu",
  "version": "1.2.0",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "lint": "npm run lint:stylint && npm run lint:tslint",
    "lint:stylint": "stylint aliemu/styles -c .stylintrc",
    "lint:tslint": "tslint -e \"node_modules/**/*\" \"**/*.+(ts|tsx)\" -t codeFrame",
    "build": "NODE_ENV=production gulp _build",
    "start": "NODE_ENV=development node --max_old_space_size=8192 gulp _dev",
    "deploy": "npm run test && npm run build && ./lib/scripts/deploy",
    "deploy-all": "npm run test && npm run build && ./lib/scripts/deploy -a",
    "sync": "./lib/scripts/sync",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "version": "gulp bump"
  },
  "dependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jest": "^20.0.0",
    "@types/react": "^15.0.29",
    "@types/react-addons-test-utils": "^0.14.18",
    "@types/react-datepicker": "^0.46.0",
    "@types/react-dom": "^15.5.0",
    "autoprefixer-stylus": "^0.14.0",
    "mobx": "^3.1.15",
    "mobx-react": "^4.2.1",
    "moment": "^2.17.1",
    "react": "^15.6.1",
    "react-datepicker": "^0.48.0",
    "react-dom": "^15.6.1",
    "typescript": "^2.3.4"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "browser-sync": "^2.18.12",
    "css-loader": "^0.28.4",
    "del": "^3.0.0",
    "enzyme": "^2.8.2",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-insert": "^0.5.0",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "jest-cli": "^20.0.4",
    "merge-stream": "^1.0.0",
    "mobx-react-devtools": "^4.2.13",
    "react-addons-test-utils": "^15.6.0",
    "style-loader": "^0.18.2",
    "stylint": "^1.5.9",
    "ts-loader": "^2.1.0",
    "tslint": "^5.4.3",
    "tslint-microsoft-contrib": "^5.0.0",
    "tslint-react": "^3.0.0",
    "webpack": "^2.6.1",
    "webpack-bundle-analyzer": "^2.8.2",
    "webpack-stream": "^3.2.0"
  },
  "jest": {
    "transform": {
      ".*": "./lib/utils/jestPreprocessor.js"
    },
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "verbose": true,
    "collectCoverageFrom": [
      "aliemu/**/*.{ts,tsx}",
      "!aliemu/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ]
  }
}
