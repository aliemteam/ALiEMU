{
  "name": "aliemu",
  "version": "1.0.1",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "lint": "npm run lint:stylint && npm run lint:tslint && npm run lint:eslint",
    "lint:stylint": "stylint aliemu/styles -c .stylintrc",
    "lint:tslint": "tslint -e \"node_modules/**/*\" \"**/*.+(ts|tsx)\" -t codeFrame",
    "lint:eslint": "eslint \"aliemu/**/*.js\" \"./*.js\" --ignore-pattern \"aliemu/vendor/*\"",
    "build": "NODE_ENV=production node_modules/.bin/gulp _build",
    "start": "NODE_ENV=development node --max_old_space_size=8192 node_modules/.bin/gulp _dev",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "dependencies": {
    "@types/enzyme": "^2.7.1",
    "@types/jest": "^16.0.3",
    "@types/moment": "^2.13.0",
    "@types/react": "^15.0.0",
    "@types/react-addons-test-utils": "^0.14.16",
    "@types/react-datepicker": "^0.28.6",
    "@types/react-dom": "^0.14.20",
    "autoprefixer-stylus": "^0.13.0",
    "mobx": "^3.0.0",
    "mobx-react": "^4.1.0",
    "moment": "^2.17.1",
    "react": "^15.4.2",
    "react-datepicker": "^0.39.0",
    "react-dom": "^15.4.2",
    "typescript": "next"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "browser-sync": "^2.18.6",
    "coveralls": "^2.11.14",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "enzyme": "^2.7.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-csso": "^2.0.0",
    "gulp-imagemin": "^3.1.1",
    "gulp-insert": "^0.5.0",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^2.3.1",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^2.0.0",
    "jest-cli": "^18.1.0",
    "merge-stream": "^1.0.0",
    "mobx-react-devtools": "^4.2.11",
    "react-addons-test-utils": "^15.4.2",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.9",
    "ts-loader": "^1.3.3",
    "tslint": "^4.3.1",
    "tslint-microsoft-contrib": "^4.0.0",
    "tslint-react": "^2.3.0",
    "webpack": "beta",
    "webpack-bundle-analyzer": "^2.2.1",
    "webpack-stream": "^3.2.0"
  },
  "jest": {
    "transform": {
      ".*": "./lib/utils/jestPreprocessor.js"
    },
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "automock": false,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "verbose": true,
    "coverageDirectory": "lib/tmp/coverage",
    "collectCoverageFrom": [
      "aliemu/**/*.{ts,tsx}",
      "!aliemu/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ]
  }
}
