{
  "name": "aliemu",
  "version": "1.4.0",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "lint": "npm run lint:stylint && npm run lint:tslint",
    "lint:stylint": "stylint aliemu/styles -c .stylintrc",
    "lint:tslint": "tslint -p . -t codeFrame",
    "build": "NODE_ENV=production gulp",
    "start": "gulp",
    "deploy": "npm run test && npm run build && ./lib/scripts/deploy",
    "deploy-all": "npm run test && npm run build && ./lib/scripts/deploy -a",
    "sync": "./lib/scripts/sync",
    "pretest": "npm run lint",
    "test": "jest --coverage",
    "version": "gulp bump"
  },
  "dependencies": {
    "mobx": "^3.2.1",
    "mobx-react": "^4.2.2",
    "moment": "^2.17.1",
    "react": "^15.6.1",
    "react-datepicker": "^0.53.0",
    "react-dom": "^15.6.1",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/enzyme": "^2.8.6",
    "@types/gulp": "^4.0.4",
    "@types/gulp-sourcemaps": "0.0.31",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.26",
    "@types/react": "^16.0.5",
    "@types/react-addons-test-utils": "^0.14.19",
    "@types/react-datepicker": "^0.52.2",
    "@types/react-dom": "^15.5.4",
    "@types/webpack": "^3.0.10",
    "autoprefixer-stylus": "^0.14.0",
    "awesome-typescript-loader": "^3.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.22.0",
    "browser-sync": "^2.18.12",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "gulp": "gulpjs/gulp#4.0",
    "gulp-imagemin": "^3.3.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-stylus": "^2.6.0",
    "gulp-uglify": "^3.0.0",
    "jest-cli": "^20.0.4",
    "merge-stream": "^1.0.0",
    "mobx-react-devtools": "^4.2.15",
    "react-addons-test-utils": "^15.6.0",
    "style-loader": "^0.18.2",
    "stylint": "^1.5.9",
    "ts-jest": "^20.0.13",
    "ts-node": "^3.2.0",
    "tslint": "^5.7.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "tslint-react": "^3.0.0",
    "uglify-es": "^3.0.28",
    "webpack": "^3.5.5",
    "webpack-bundle-analyzer": "^2.8.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "aliemu/**/*.{ts,tsx}",
      "!aliemu/aliemu.d.ts",
      "!aliemu/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ],
    "coverageDirectory": "lib/tmp/coverage",
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
