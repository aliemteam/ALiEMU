{
  "name": "aliemu",
  "version": "1.9.0",
  "license": "MIT",
  "repository": "https://github.com/aliemteam/ALiEMU",
  "server": {
    "name": "aliemu",
    "plugins": [
      "learndash-propanel",
      "sfwd-lms"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "deploy-all": "npm run test && npm run build && ./lib/scripts/deploy.py -a",
    "deploy": "npm run test && npm run build && ./lib/scripts/deploy.py",
    "lint:scss": "stylelint **/*.scss",
    "lint:ts": "tslint -p . -t codeFrame",
    "lint:php": "npm run phpcs -- --ignore=src/learndash --report=${PHPCS_REPORT:-summary} ./src",
    "phpcs": "docker run -it --rm -v $(pwd):/app willhallonline/wordpress-phpcs phpcs",
    "lint": "npm run lint:scss && npm run lint:ts && npm run lint:php",
    "pretest": "PHPCS_REPORT=full npm run lint",
    "pull-subtree": "git subtree pull -P lib/scripts --squash devops master",
    "start": "webpack",
    "sync": "./lib/scripts/sync.py",
    "test": "jest --coverage",
    "version": "ts-node ./lib/utils/version.ts && git add -A",
    "up": "docker-compose up -d && docker-compose logs -f wordpress",
    "down": "docker-compose down -v"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "@types/classnames": "^2.2.3",
    "classnames": "^2.2.5",
    "mobx": "^5.0.0",
    "mobx-react": "^5.1.2",
    "moment": "^2.22.1",
    "normalize.css": "^8.0.0",
    "react": "^16.3.2",
    "react-datepicker": "^1.4.1",
    "react-dom": "^16.3.2",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.37",
    "@babel/preset-env": "^7.0.0-beta.37",
    "@babel/preset-react": "^7.0.0-beta.37",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/enzyme": "^3.1.10",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^22.2.3",
    "@types/jquery": "^3.3.1",
    "@types/node": "^10.0.3",
    "@types/react": "^16.3.13",
    "@types/react-addons-test-utils": "^0.14.21",
    "@types/react-datepicker": "^1.1.2",
    "@types/react-dom": "^16.0.5",
    "@types/webpack": "^4.1.4",
    "autoprefixer": "^8.4.1",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.0-beta.0",
    "browser-sync": "^2.24.3",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "imagemin": "^5.3.1",
    "imagemin-pngquant": "^5.1.0",
    "imagemin-svgo": "^6.0.0",
    "jest-cli": "^22.4.3",
    "mini-css-extract-plugin": "^0.4.0",
    "mobx-react-devtools": "^5.0.1",
    "node-sass": "^4.9.0",
    "react-addons-test-utils": "^15.6.2",
    "react-svg-loader": "^2.1.0",
    "sass-loader": "^7.0.1",
    "snapshot-diff": "^0.3.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.1.0",
    "ts-jest": "^22.4.4",
    "ts-node": "^6.0.3",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.12.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.5.1",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.1.3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/_entrypoints/*",
      "!src/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ],
    "coverageDirectory": "node_modules/.cache/coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "./lib/utils/before-test.ts"
    ],
    "setupTestFrameworkScriptFile": "./lib/utils/setup-test.ts",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  }
}
