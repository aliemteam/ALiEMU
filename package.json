{
  "name": "aliemu",
  "version": "1.9.0",
  "license": "MIT",
  "repository": "https://github.com/aliemteam/ALiEMU",
  "server": {
    "name": "aliemu",
    "plugins": [
      "learndash-propanel",
      "sfwd-lms"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "deploy-all": "npm run test && npm run build && ./lib/scripts/deploy.py -a",
    "deploy": "npm run test && npm run build && ./lib/scripts/deploy.py",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:ts": "tslint -p . -t stylish",
    "lint:php": "npm run phpcs -- --ignore=src/learndash --report=${PHPCS_REPORT:-summary} --basepath=. ./src",
    "phpcs": "docker run -it --rm -v $(pwd):/app dsifford/phpcs",
    "lint": "npm run lint:scss && npm run lint:ts && npm run lint:php",
    "pretest": "PHPCS_REPORT=full npm run lint",
    "pull-subtree": "git subtree pull -P lib/scripts --squash devops master",
    "start": "webpack",
    "sync": "./lib/scripts/sync.py",
    "test": "jest --coverage",
    "version": "ts-node ./lib/utils/version.ts && git add -A",
    "view-coverage": "$(test $(uname) == Linux && echo xdg-open || echo open) ./node_modules/.cache/coverage/lcov-report/index.html",
    "up": "docker-compose up -d && docker-compose logs -f wordpress",
    "down": "docker-compose down -v"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "@types/classnames": "^2.2.6",
    "classnames": "^2.2.6",
    "common-tags": "^1.8.0",
    "datalist-polyfill": "^1.20.1",
    "date-fns": "^1.29.0",
    "material-design-icons": "^3.0.1",
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.3",
    "mobx-utils": "^5.0.0",
    "normalize.css": "^8.0.0",
    "react": "^16.4.2",
    "react-content-loader": "^3.1.2",
    "react-dom": "^16.4.2",
    "striptags": "^3.1.1",
    "typescript": "^3.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@types/common-tags": "^1.4.0",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/enzyme": "^3.1.12",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.1",
    "@types/jquery": "^3.3.5",
    "@types/node": "^10.5.5",
    "@types/react": "^16.4.7",
    "@types/react-addons-test-utils": "^0.14.21",
    "@types/react-content-loader": "^3.1.2",
    "@types/react-dom": "^16.0.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/webpack": "^4.4.9",
    "autoprefixer": "^9.1.0",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0-beta.3",
    "browser-sync": "^2.24.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "^6.0.0",
    "imagemin-pngquant": "^6.0.0",
    "imagemin-svgo": "^7.0.0",
    "jest": "^23.4.2",
    "mini-css-extract-plugin": "^0.4.1",
    "mobx-react-devtools": "^6.0.2",
    "node-sass": "^4.9.2",
    "prettier": "^1.14.0",
    "react-addons-test-utils": "^15.6.2",
    "react-svg-loader": "^2.1.0",
    "react-test-renderer": "^16.4.2",
    "sass-loader": "^7.1.0",
    "snapshot-diff": "^0.4.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.4.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.2.0",
    "ts-jest": "^23.1.2",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-microsoft-contrib": "^5.1.0",
    "tslint-react": "^3.6.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json"
      }
    },
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ],
    "coverageDirectory": "node_modules/.cache/coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testURL": "http://localhost",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  }
}
