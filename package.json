{
  "name": "aliemu",
  "version": "1.0.0",
  "description": "Development taskrunner pipeline",
  "author": "Derek P Sifford",
  "license": "MIT",
  "repository": "https://github.com/AcademicLifeInEM/ALiEMU",
  "scripts": {
    "deploy": "./lib/scripts/deploy",
    "sync": "./lib/scripts/syncup",
    "minify": "./lib/scripts/minify",
    "wp": "wpcmd() { docker exec aliemu_wordpress_1 /bin/bash -c \"sudo -u www-data wp $(echo $@)\"; };wpcmd ",
    "rollback": "if [[ $(git status --porcelain) ]]; then git stash; fi && git checkout master && git fetch && git pull && git revert HEAD --no-edit && npm run deploy && git stash pop",
    "lint": "stylint aliemu/styles -c .stylintrc && tslint 'aliemu/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "test": "jest --silent"
  },
  "dependencies": {
    "@types/enzyme": "^2.4.30",
    "@types/jest": "^0.9.28",
    "@types/moment": "^2.11.29",
    "@types/react": "^0.14.28",
    "@types/react-addons-test-utils": "^0.14.14",
    "@types/react-dom": "^0.14.14",
    "@types/sinon": "^1.16.27",
    "moment": "^2.14.1",
    "react": "^15.3.0",
    "react-datepicker": "^0.28.2",
    "react-dom": "^15.3.0",
    "typescript": "^2.1.0-dev.20160714"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.14.0",
    "coveralls": "^2.11.12",
    "css-loader": "^0.23.1",
    "del": "^2.2.1",
    "enzyme": "^2.4.1",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-csso": "^2.0.0",
    "gulp-insert": "^0.5.0",
    "gulp-jade2php": "^1.0.3",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-stylus": "^2.5.0",
    "gulp-svgmin": "^1.2.2",
    "gulp-uglify": "^2.0.0",
    "jest-cli": "^14.1.0",
    "merge-stream": "^1.0.0",
    "poststylus": "^0.2.3",
    "react-addons-test-utils": "^15.3.0",
    "rucksack-css": "^0.8.6",
    "sinon": "^1.17.5",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.6",
    "ts-loader": "^0.8.1",
    "tslint": "^3.14.0",
    "webpack": "^1.13.1",
    "webpack-stream": "^3.1.0"
  },
  "jest": {
    "scriptPreprocessor": "./lib/utils/jestPreprocessor.js",
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme",
      "sinon",
      "<rootDir>/lib/utils/Fixtures"
    ],
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "tmp/coverage"
  }
}
