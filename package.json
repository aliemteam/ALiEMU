{
  "name": "aliemu",
  "version": "1.6.0",
  "license": "MIT",
  "repository": "https://github.com/aliemteam/ALiEMU",
  "server": {
    "name": "aliemu",
    "base_theme": "Divi",
    "plugins": [
      "learndash-propanel",
      "sfwd-lms"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production gulp",
    "deploy-all": "npm run test && npm run build && ./lib/scripts/deploy -a",
    "deploy": "npm run test && npm run build && ./lib/scripts/deploy",
    "lint": "tslint -p . -t codeFrame",
    "pretest": "npm run lint",
    "pull-subtree": "git subtree pull -P lib/scripts --squash devops master",
    "start": "gulp",
    "sync": "./lib/scripts/sync",
    "test": "jest --coverage",
    "version": "gulp bump",
    "up": "docker-compose up -d && docker-compose logs -f wordpress",
    "down": "docker-compose down -v"
  },
  "dependencies": {
    "mobx": "^3.4.1",
    "mobx-react": "^4.3.5",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-datepicker": "^1.0.4",
    "react-dom": "^16.2.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.6",
    "@types/gulp": "^4.0.5",
    "@types/gulp-autoprefixer": "0.0.31",
    "@types/gulp-sass": "0.0.31",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/jest": "^22.0.1",
    "@types/jquery": "^3.2.17",
    "@types/node": "^9.3.0",
    "@types/react": "^16.0.34",
    "@types/react-addons-test-utils": "^0.14.20",
    "@types/react-datepicker": "^1.0.0",
    "@types/react-dom": "^16.0.3",
    "@types/webpack": "^3.8.2",
    "autoprefixer": "^7.2.4",
    "awesome-typescript-loader": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.22.0",
    "browser-sync": "^2.23.5",
    "css-loader": "^0.28.8",
    "enzyme": "^3.3.0",
    "gulp": "^4.0.0",
    "gulp-autoprefixer": "^4.1.0",
    "gulp-imagemin": "^4.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.3",
    "gulp-uglify": "^3.0.0",
    "jest-cli": "^22.0.5",
    "merge-stream": "^1.0.0",
    "mobx-react-devtools": "^4.2.15",
    "react-addons-test-utils": "^15.6.2",
    "style-loader": "^0.19.1",
    "ts-jest": "^22.0.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "tslint-react": "^3.3.3",
    "uglify-es": "^3.3.5",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "aliemu/**/*.{ts,tsx}",
      "!aliemu/aliemu.d.ts",
      "!aliemu/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ],
    "coverageDirectory": "lib/tmp/coverage",
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "mapCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testRegex": "__tests__/.*\\.(js|ts|tsx)$",
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
