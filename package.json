{
  "name": "aliemu",
  "version": "1.9.0",
  "license": "MIT",
  "repository": "https://github.com/aliemteam/ALiEMU",
  "server": {
    "name": "aliemu",
    "plugins": [
      "learndash-propanel",
      "sfwd-lms"
    ]
  },
  "scripts": {
    "build": "NODE_ENV=production TS_NODE_FILES=true webpack",
    "deploy-all": "npm run test && npm run build && ./lib/scripts/deploy.py -a",
    "deploy": "npm run test && npm run build && ./lib/scripts/deploy.py",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:ts": "tslint -p . -t stylish",
    "lint:php": "npm run phpcs -- --ignore=src/learndash --report=${PHPCS_REPORT:-summary} --basepath=. ./src",
    "phpcs": "docker run -it --rm -v $(pwd):/app dsifford/phpcs",
    "lint": "npm run lint:scss && npm run lint:ts && npm run lint:php",
    "pretest": "PHPCS_REPORT=full npm run lint",
    "pull-subtree": "git subtree pull -P lib/scripts --squash devops master",
    "start": "TS_NODE_FILES=true webpack",
    "sync": "./lib/scripts/sync.py",
    "test": "jest --coverage",
    "version": "ts-node ./lib/utils/version.ts && git add -A",
    "up": "docker-compose up -d && docker-compose logs -f wordpress",
    "down": "docker-compose down -v"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.49",
    "@types/classnames": "^2.2.4",
    "classnames": "^2.2.6",
    "datalist-polyfill": "^1.16.1",
    "date-fns": "^1.29.0",
    "material-design-icons": "^3.0.1",
    "mobx": "^4.3.1",
    "mobx-react": "^5.2.3",
    "mobx-utils": "^5.0.0",
    "normalize.css": "^8.0.0",
    "react": "^16.4.1",
    "react-content-loader": "^3.1.2",
    "react-dom": "^16.4.1",
    "striptags": "^3.1.1",
    "typescript": "^2.9.2",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@types/copy-webpack-plugin": "^4.4.1",
    "@types/enzyme": "^3.1.11",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.1.3",
    "@types/jquery": "^3.3.4",
    "@types/node": "^10.5.1",
    "@types/react": "^16.4.5",
    "@types/react-addons-test-utils": "^0.14.21",
    "@types/react-content-loader": "^3.1.2",
    "@types/react-dom": "^16.0.6",
    "@types/webpack": "^4.4.4",
    "autoprefixer": "^8.6.4",
    "awesome-typescript-loader": "^5.2.0",
    "babel-loader": "^8.0.0-beta.3",
    "browser-sync": "^2.24.5",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "imagemin": "^5.3.1",
    "imagemin-pngquant": "^5.1.0",
    "imagemin-svgo": "^6.0.0",
    "jest-cli": "^23.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "mobx-react-devtools": "^5.0.1",
    "node-sass": "^4.9.0",
    "react-addons-test-utils": "^15.6.2",
    "react-svg-loader": "^2.1.0",
    "sass-loader": "^7.0.3",
    "snapshot-diff": "^0.4.0",
    "style-loader": "^0.21.0",
    "stylelint": "^9.3.0",
    "stylelint-config-recommended-scss": "^3.2.0",
    "stylelint-scss": "^3.1.3",
    "ts-jest": "^22.4.6",
    "ts-node": "^7.0.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.13.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "tslint-react": "^3.6.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/_entrypoints/*",
      "!src/**/index.{ts,tsx}",
      "!**/{vendor,__tests__}/**"
    ],
    "coverageDirectory": "node_modules/.cache/coverage",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "setupFiles": [
      "./lib/utils/before-test.ts"
    ],
    "setupTestFrameworkScriptFile": "./lib/utils/setup-test.ts",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    }
  }
}
